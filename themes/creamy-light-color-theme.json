  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#A0A3A8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#16A34A"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#EA580C"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#D946EF"
      }
    },
    {
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#9333EA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#575AFD",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator", "keyword.operator.new"],
      "settings": {
        "foreground": "#DC2626"
      }
    },
    {
      "scope": ["keyword.control.flow", "keyword.control.return", "keyword.control.conditional"],
      "settings": {
        "foreground": "#9333EA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#2563EB",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#2F3137"
      }
    },
    {
      "scope": ["variable.language", "variable.parameter"],
      "settings": {
        "foreground": "#EA580C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#9333EA"
      }
    },
    {
      "scope": ["entity.name.class", "support.class"],
      "settings": {
        "foreground": "#2563EB",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.namespace", "entity.name.module"],
      "settings": {
        "foreground": "#9333EA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["punctuation.definition", "punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#64748B"
      }
    },
    {
      "scope": ["punctuation.bracket", "meta.bracket"],
      "settings": {
        "foreground": "#475569"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#575AFD"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#EA580C"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#2563EB",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#16A34A"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#2F3137",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#2F3137",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#EA580C"
      }
    },
    {
      "scope": ["markup.link"],
      "settings": {
        "foreground": "#575AFD"
      }
    },
    {
      "scope": ["support.constant.property-value"],
      "settings": {
        "foreground": "#16A34A"
      }
    },
    {
      "scope": ["meta.object-literal.key", "variable.object.property"],
      "settings": {
        "foreground": "#DC2626"
      }
    },
    {
      "scope": ["meta.decorator", "meta.decorator variable.function"],
      "settings": {
        "foreground": "#D946EF",
        "fontStyle": "italic"
      }
    }
  ]
}